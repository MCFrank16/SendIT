{
  "name": "sendit-develop",
  "version": "1.0.0",
  "description": "SendIT Courier Delivery App",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require babel-register Test/**/*.js --exit",
    "build": "babel server.js --out-dir build",
    "dev-start": "babel-watch server.js && SET NODE_ENV=dev",
    "lint": "eslint .",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover mocha Test/Parcel.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "cover": "./node_modules/istanbul/cli cover _mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MCFrank16/SendIT.git"
  },
  "keywords": [
    "SendIT",
    "Andela",
    "Challenge"
  ],
  "author": "Frank C. Mutabazi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MCFrank16/SendIT/issues"
  },
  "homepage": "https://github.com/MCFrank16/SendIT#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "chai-uuid": "^1.0.6",
    "express": "^4.16.4",
    "uuid": "^3.3.2"
  }
}
